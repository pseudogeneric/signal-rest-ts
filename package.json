{
  "name": "signal-web",
  "version": "0.0.1",
  "description": "ts wrapper around signal-cli-rest-api",
  "license": "MIT",
  "author": "Grant Bacon",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@g.l5.ca:2222/srv/git/signal-web.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "jest",
    "build": "node build.mjs",
    "types": "tsc -p tsconfig.json --emitDeclarationOnly",
    "clean": "rm -rf dist/",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "esbuild": "0.25.5",
    "git-format-staged": "^3.1.1",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tslib": "^2.8.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  }
}
